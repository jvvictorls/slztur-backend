{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build",
    "start": "ts-node src/server.ts",
    "db:reset": "npx -y tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "dev": "tsnd --watch \"./src/**\" ./src/server.ts",
    "test": "mocha -r ts-node/register ./tests/**/*$NAME*.test.ts -t 10000 --exit",
    "lint": "tsc && eslint . --ext .ts",
    "test:coverage": "nyc --all --include src/models --include src/services --include src/controllers npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.14",
    "sequelize": "^6.25.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "16.11.31",
    "@types/sequelize": "^4.28.14",
    "eslint": "6.8.0",
    "eslint-config-trybe-backend": "1.0.1",
    "sequelize-cli": "6.6.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
